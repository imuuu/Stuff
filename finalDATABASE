-- Dumping structure for taulu finalrefuge.item
CREATE TABLE IF NOT EXISTS `item` (
  `id` int(11) NOT NULL,
  `type_id` int(11) NOT NULL,
  `x` int(11) DEFAULT NULL,
  `y` int(11) DEFAULT NULL,
  `player_ID` int(11) DEFAULT NULL,
  `equipped` int(11) DEFAULT NULL,
  `object_ID` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `type_id` (`type_id`),
  KEY `x` (`x`,`y`),
  KEY `player_ID` (`player_ID`),
  KEY `item_ibfk_4` (`object_ID`),
  CONSTRAINT `item_ibfk_1` FOREIGN KEY (`type_id`) REFERENCES `item_type` (`id`),
  CONSTRAINT `item_ibfk_2` FOREIGN KEY (`x`, `y`) REFERENCES `terrain_square` (`x`, `y`),
  CONSTRAINT `item_ibfk_3` FOREIGN KEY (`player_ID`) REFERENCES `player` (`ID`),
  CONSTRAINT `item_ibfk_4` FOREIGN KEY (`object_ID`) REFERENCES `object` (`Id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

