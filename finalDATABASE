-- Dumping structure for taulu finalrefuge.enemy
CREATE TABLE IF NOT EXISTS `enemy` (
  `id` int(11) NOT NULL,
  `type_id` int(11) NOT NULL,
  `x` int(11) DEFAULT NULL,
  `y` int(11) DEFAULT NULL,
  `spawned` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `type_id` (`type_id`),
  KEY `x` (`x`,`y`),
  CONSTRAINT `enemy_ibfk_1` FOREIGN KEY (`type_id`) REFERENCES `enemy_type` (`id`),
  CONSTRAINT `enemy_ibfk_2` FOREIGN KEY (`x`, `y`) REFERENCES `terrain_square` (`x`, `y`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

