-- Dumping structure for taulu finalrefuge.object
CREATE TABLE IF NOT EXISTS `object` (
  `Id` int(11) NOT NULL,
  `name` varchar(50) DEFAULT NULL,
  `x` int(11) DEFAULT NULL,
  `y` int(11) DEFAULT NULL,
  `key_item_id` int(11) DEFAULT NULL,
  `open` int(11) DEFAULT NULL,
  `storable` int(1) DEFAULT NULL,
  `player_id` int(1) DEFAULT NULL,
  PRIMARY KEY (`Id`),
  KEY `x` (`x`,`y`),
  KEY `key_item_id` (`key_item_id`),
  KEY `player_id` (`player_id`),
  CONSTRAINT `object_ibfk_1` FOREIGN KEY (`x`, `y`) REFERENCES `terrain_square` (`x`, `y`),
  CONSTRAINT `object_ibfk_2` FOREIGN KEY (`key_item_id`) REFERENCES `item_type` (`id`),
  CONSTRAINT `object_ibfk_3` FOREIGN KEY (`player_id`) REFERENCES `player` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

