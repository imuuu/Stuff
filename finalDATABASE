-- Dumping structure for taulu finalrefuge.player
CREATE TABLE IF NOT EXISTS `player` (
  `ID` int(11) NOT NULL,
  `name` varchar(40) NOT NULL,
  `carry` int(11) NOT NULL,
  `att` int(11) DEFAULT NULL,
  `speed` decimal(10,0) DEFAULT NULL,
  `hp` int(11) NOT NULL,
  `fatigue` int(11) NOT NULL,
  `x` int(11) NOT NULL,
  `y` int(11) NOT NULL,
  `infection` int(3) NOT NULL DEFAULT 0,
  PRIMARY KEY (`ID`),
  KEY `x` (`x`,`y`),
  CONSTRAINT `player_ibfk_1` FOREIGN KEY (`x`, `y`) REFERENCES `terrain_square` (`x`, `y`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

